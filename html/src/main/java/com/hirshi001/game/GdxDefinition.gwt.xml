<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.2//EN" "http://www.gwtproject.org/doctype/2.8.2/gwt-module.dtd">
<module rename-to="html">
    <source path="" />

	<inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
    <inherits name="com.badlogic.gdx.ai" />
    <inherits name="com.dongbat.jbump" />
    <inherits name="com.badlogic.gdx.graphics.g2d.freetype.freetype-gwt" />
    <inherits name="com.ray3k.stripe" />
    <inherits name="com.ray3k.tenpatch.tenpatch"/>
    <inherits name="regexodus.regexodus" />
    <inherits name="com.github.tommyettinger.textratypist" />
	<inherits name="com.hirshi001.game.GameApp" />
    <inherits name="com.hirshi001.game.shared.shared" />


    <inherits name="com.hirshi001.networking.NetworkingLibrary" />
    <inherits name="com.hirshi001.gwtrestapi.gwtrestapi" />
    <inherits name="com.hirshi001.restapi.restapi" />
    <inherits name="com.hirshi001.gwtnetworking.gwtnetworking" />
    <inherits name="com.hirshi001.buffer.buffer" />

    <inherits name="com.google.gwt.user.User" />
    <inherits name="com.google.gwt.core.Core" />
    <inherits name='com.github.czyzby.websocket.GdxWebSocketGwt' />

    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.audio.Sound" />
    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.audio.Music" />
    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.g2d.BitmapFont" />

    <entry-point class="com.hirshi001.game.gwt.GwtLauncher" />

    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
    <!-- These two lines reduce the work GWT has to do during compilation and also shrink output size. -->
    <set-property name="user.agent" value="gecko1_8, safari"/>
    <collapse-property name="user.agent" values="*" />
    <!-- Remove the "user.agent" lines above if you encounter issues with Safari or other Gecko browsers. -->
</module>
